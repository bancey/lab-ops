#cloud-config
autoinstall:
  version: 1
  locale: en_GB
  keyboard:
    layout: en
  ssh:
    install-server: true
    allow-pw: true
    disable_root: true
  user-data:
    timezone: Europe/London
    users:
      - name: $(VM-Template-Username)
        groups: [adm, sudo]
        lock-passwd: false
        sudo: ALL=(ALL) NOPASSWD:ALL
        shell: /bin/bash
        passwd: "$(VM-Template-SHA256-Password)"
        ssh_authorized-keys:
          - ssh-rsa  alexb@Bancey-W11
          - ssh-rsa  alexb@DESKTOP-CN2369K
          - $(Packer-Public-Key)
